{
  "name": "swimmeetscore",
  "version": "1.5.6",
  "private": true,
  "scripts": {
    "build": "npm run build:css && npm run build:js",
    "build:js": "babel app.jsx --presets=@babel/preset-react | terser --compress --mangle reserved=['APP_VERSION'] -o app.js",
    "build:css": "tailwindcss -i input.css -o app.css --minify",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:static": "node tests/app-logic.test.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.5",
    "@babel/core": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "jest": "^29.7.0",
    "tailwindcss": "^3.4.0",
    "terser": "^5.46.0"
  }
}
